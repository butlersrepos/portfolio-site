	<header id="resume-title" class="small-12 column">
		<div id="resume-title-name" class="small-4 medium-8 columns">Eric Butler</div>
		<div id="resume-title-info" class="small-8 medium-4 columns">
			<div class="address-line">Columbus, OH</div>
			<div class="address-line">ericbutler000@gmail.com</div>
			<div class="address-line">(419)-366-9274</div>
		</div>
	</header>
	<section id="resume-body" class="row collapse">
		<div id="resume-body-left" class="small-12 medium-9 columns">
			<div class="resume-header">EDUCATION</div>
			<div class="row resume-date-entry">
				<div class="small-12 columns resume-event">Economics, Master of Arts</div>
