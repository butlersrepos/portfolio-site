			</div>

			<div id="experience-title" class="resume-header">EXPERIENCE</div>

			<div id="employers">
				{{ for( var e in resume.bio_data.experience.positions ) { }}
					{{ var employer = resume.bio_data.experience.positions[e]; }}
					{{ include ../templates/employer-entry.ejs }}
				{{ } }}
			</div>

			{{ for( var p in resume.bio_data.experience.projects ) { }}
				{{ var entry = resume.bio_data.experience.projects[p]; }}
				{{ include ../templates/assignment-entry.ejs }}
			{{ } }}
		</div>
		<section id="resumeBodyRight" class="small-12 medium-3 columns">
			<h2 id="skills-title">Skills &amp; Techs</h2>
			<div class="row">
