{
  "name": "nodejs-portfolio-site",
  "version": "0.0.2",
  "private": true,
  "dependencies": {
    "body-parser": "^1.12.3",
    "consolidate": "^0.12.1",
    "cp": "~0.2.0",
    "ejs": "*",
    "errorhandler": "^1.3.5",
    "express": "^4.12.3",
    "jade": "^1.9.2",
    "jquery": "^3.1.0",
    "method-override": "^2.3.2",
    "mkdirp": "~0.5.1",
    "moment": "~2.5.1",
    "morgan": "^1.5.2",
    "serve-favicon": "^2.2.0"
  },
  "devDependencies": {
    "bower": "~1.3.12",
    "browser-sync": "~2.2.1",
    "del": "~1.1.1",
    "gulp": "~3.8.10",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-concat": "~2.5.1",
    "gulp-cssnano": "^2.0.0",
    "gulp-debug": "~2.0.0",
    "gulp-filter": "~2.0.2",
    "gulp-if": "~1.2.5",
    "gulp-jshint": "~1.9.2",
    "gulp-load-plugins": "~0.8.0",
    "gulp-nodemon": "~1.0.5",
    "gulp-sass": "^2.1.0",
    "gulp-uglify": "~1.1.0",
    "jshint-stylish": "~1.0.0",
    "lodash": "^4.15.0",
    "purecss": "^0.6.0",
    "require-dir": "~0.1.0",
    "rollup": "^0.34.10",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "stream-series": "^0.1.1"
  },
  "scripts": {
    "build": "npm run build-js",
    "build-imgs": "mkdirp dist/imgs && cp -r src/imgs/* dist/imgs/",
    "build-fonts": "mkdirp dist/fonts && cp -r src/fonts/* dist/fonts/",
    "build-js": "rollup --config rollup.config.js",
    "build-css": "node-sass src/scss/bundled.scss --output dist/css",
    "predeploy": "git pull && forever stop mainServer.js &",
    "deploy": "npm install && npm run build && gulp deploy",
    "postdeploy": "forever start mainServer.js",
    "start": "node mainServer.js",
    "dev": "",
    "watch-css": "node-sass src/scss/bundled.scss --output dist/css --watch"
  }
}
